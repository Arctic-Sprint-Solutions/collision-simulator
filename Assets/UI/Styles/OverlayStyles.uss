.root-container {
  position: relative;
  width: 100%; height: 100%;
}

.keybinds-edge {
  position: absolute;
  top: -20px; left:800px ; right: 800px;
  height: 50px;
  background-color: transparent;
}

.keybinds-panel {
  position: absolute;
  top: -220px;
  left: 0; right: 0;
  height: 200px;
  background-color: rgba(0, 0, 0, 0.9);
}

.keybind-row {
  padding: 4px 8px;
  color: white;
}
